{
  "name": "vtkjs-viewport",
  "version": "0.0.1",
  "description": "Viewport and annotation framework for vtk.js",
  "author": "OHIF Contributors",
  "license": "MIT",
  "repository": "OHIF/react-vtkjs-viewport",
  "main": "dist/index.umd.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "webpack --progress --config ./.webpack/webpack.config.js --mode development",
    "build:release": "webpack --progress --config ./.webpack/webpack.config.js --mode production",
    "build:examples": "webpack --progress --config ./.webpack/webpack.dev.js",
    "dev": "webpack serve --config ./.webpack/webpack.dev.js --watch",
    "preinstall": "node preinstall.js",
    "start": "yarn run dev",
    "test": "karma start",
    "test:ci": "karma start --single-run",
    "lint-staged": "lint-staged",
    "lint": "eslint -c .eslintrc.json src",
    "predeploy": "yarn install && yarn run build:release",
    "prepublishOnly": "yarn run build && yarn run build:release"
  },
  "peerDependencies": {
    "calculate-suv": "git+ssh://git@github.com/PrecisionMetrics/calculate-suv.git#main",
    "cornerstone-math": "0.1.9",
    "cornerstone-wado-image-loader": "git+ssh://git@github.com/PrecisionMetrics/cornerstoneWADOImageLoader.git#merge-suv-calculation",
    "vtk.js": "https://github.com/swederik/vtk-js.git#perf/md5-built"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "^4.5.0",
    "ts-loader": "^8.0.12",
    "typescript": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@semantic-release/npm": "7.0.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "calculate-suv": "git+ssh://git@github.com/PrecisionMetrics/calculate-suv.git#main",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cornerstone-core": "^2.3.0",
    "cornerstone-math": "^0.1.9",
    "cornerstone-wado-image-loader": "git+ssh://git@github.com/PrecisionMetrics/cornerstoneWADOImageLoader.git#merge-suv-calculation",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dcmjs": "^0.17.0",
    "dicom-parser": "^1.8.7",
    "dicomweb-client": "^0.6.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.x",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "jasmine": "^3.6.3",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-webpack": "^5.0.0-alpha.5",
    "kw-web-suite": "^11.0.0",
    "lint-staged": "^10.5.3",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-resize-detector": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "semantic-release": "17.3.0",
    "shader-loader": "^1.3.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
